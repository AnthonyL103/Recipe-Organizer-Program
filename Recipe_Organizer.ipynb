        "#import random here for the random recipe function\n",
        "import random\n",
        "#Class recipe is defined here with all the necessary attributes/variables of recipes\n",
        "#the attributes that are defined are name, cuisine, ingredients, and instructions\n",
        "class Recipe:\n",
        "  def __init__(self, name, cuisine, ingredients, instructions):\n",
        "    self.name = name\n",
        "    self.cuisine = cuisine\n",
        "    self.ingredients = ingredients\n",
        "    self.instructions = instructions\n",
        "#Class Recipe Organizer is the class with all the functions that are called upon in the main while loop\n",
        "class RecipeOrganizer:\n",
        "    #defines a list called recipes that stores all recipes added\n",
        "    def __init__(self):\n",
        "        self.recipes = []\n",
        "    #defines add_recipe function that takes the recipe as a parameter (recipe is defined in the main while loop for option 1)\n",
        "    def add_recipe(self, recipe):\n",
        "    #appends recipe to recipes list\n",
        "        self.recipes.append(recipe)\n",
        "    #defines remove_recipe function that takes in the name of the recipe which is defined as user input in main while loop\n",
        "    def remove_recipe(self, name):\n",
        "        #defines a for loop that goes through the recipe list\n",
        "        for recipe in self.recipes:\n",
        "          #if the recipe name is equal to the user inputed name then it will remove that recipe from the list\n",
        "          if recipe.name.lower() == name:\n",
        "            self.recipes.remove(recipe)\n",
        "            return True\n",
        "          else:\n",
        "            return False\n",
        "\n",
        "    #defines search_recipe_ing (search recipe by ingredients) with ingredient variable passed in as a parameter, ingredient is defines in main while loop as user input\n",
        "    def search_recipe_ing(self, ingredient):\n",
        "      #creates found_recipes list for all the recipes that are found\n",
        "      found_recipes = []\n",
        "      #for loop that goes through each recipe in recipies list\n",
        "      for recipe in self.recipes:\n",
        "        #nested for loop that goes through each ingredient of every recipe and finds a match to user inputed ingredient\n",
        "        #uses .lower() method to get rid of case sensitive situations\n",
        "         if ingredient.lower() in [i.lower() for i in recipe.ingredients]:\n",
        "          #adds recipies with matching ingredients to found_recipies list and returns found_recipies\n",
        "          found_recipes.append(recipe)\n",
        "      return found_recipes\n",
        "\n",
        "    #same as search_recipie_ing but takes in cuisine as an input in main while loop and passes it in the function as a paramter\n",
        "    def search_recipe_cuis(self, cuisine):\n",
        "      found_recipes = []\n",
        "      for recipe in self.recipes:\n",
        "        if cuisine.lower() == recipe.cuisine.lower():\n",
        "          found_recipes.append(recipe)\n",
        "      return found_recipes\n",
        "\n",
        "    #Defines get_random_recipe that generatesa random recipe\n",
        "    def get_random_recipe(self):\n",
        "      #if there is no recipes then the function will return none\n",
        "      if not self.recipes:\n",
        "        return None\n",
        "      #Otherwise the function will return a randomly generated recipe in the recipes list using the random.choice method (import random at the top)\n",
        "      return random.choice(self.recipes)\n",
        "\n",
        "    #defines recipe count function that returns the length of recipies list to see how many recipies there are (used in random recipe function in main while loop)\n",
        "    def recipe_count(self):\n",
        "      return len(self.recipes)\n",
        "\n",
        "\n",
        "\n",
        "#the function prompt_recipe() will prompt the user to input all the data of their recipe\n",
        "def prompt_recipe():\n",
        "    #prompts user to input all the data of their recipe for all the variables in recipe (name, cuisine....)\n",
        "    name = input(\"Enter your recipe name: \")\n",
        "    cuisine = input(\"Enter the cuisine of your recipe: \")\n",
        "    #for ingredients I just prompted the user to add ingredients separated by commas, then all the ingredients will be split into a list by each comma\n",
        "    ingredients = input(\"Enter your recipe ingredients (separated by commas): \").split(\",\")\n",
        "    #remove whitespaces for each typed ingredient\n",
        "    stripped_ingredients = list(map(str.strip, ingredients))\n",
        "    #for the instructions I made it so it was a while loop do the user could keep on adding instructions, and if they finish then they just have to type \"done\"\n",
        "    #once done is inputed then the loop will break, this allows for the user to add intructions without doing it all on one line\n",
        "    print(\"Enter your recipe instructions (one step per line, type done to finish):\")\n",
        "    instructions = []\n",
        "    while True:\n",
        "      #added .lower() for case sensitive situations\n",
        "      step = input().strip()\n",
        "      if step.lower() == \"done\":\n",
        "        break\n",
        "      #each instruction inputed is then added to the instructions list\n",
        "      instructions.append(step)\n",
        "      #the next message will prompt the user if they want to continue adding more recipes or exit back to the main loop\n",
        "\n",
        "    # assocciates variable recipe with the attributes of the Recipe class\n",
        "    new_recipe = Recipe(name, cuisine, stripped_ingredients, instructions)\n",
        "\n",
        "    return new_recipe\n",
        "\n",
        "#This is the print recipe function that will take in class recipe as data\n",
        "def print_recipe(recipe):\n",
        "    #the function will then print all the variables of the recipe stored in the recipe class like this (\"Recipe Variable: + correlating data in class recipe\")\n",
        "    print(\"Recipe:\", recipe.name)\n",
        "    print(\"Cuisine:\", recipe.cuisine)\n",
        "    print(\"Ingredients:\")\n",
        "    #to print ingredients a for loop that runs for however many ingredients there are, this way all ingredients are printed on a separate line with a \"-\" in front. This makes the output much cleaner\n",
        "    for ingredient in recipe.ingredients:\n",
        "        print(\"- \" + ingredient)\n",
        "    #to print the instructions, it is very similar to printing ingredients though instead it will print the number of the step by adding variable step_num to the front of the output\n",
        "    print(\"Instructions:\")\n",
        "    for instructions in recipe.instructions:\n",
        "        #step_num starts at 1\n",
        "        step_num = 1\n",
        "        print(str(step_num) + \".\", instructions)\n",
        "        #step num increases by 1 for every iteration  as the steps will go like 1. ...., 2. ....., etc\n",
        "        step_num += 1\n",
        "#main function where user is prompted main menu where they can access all functions of the RecipeOrganizer class\n",
        "#the main function is one giant while loop that continues to run unless option 6 is selected which will exit the program via break\n",
        "#the input is taken as a number from 1-6(each numer correlating with a certain function)\n",
        "#the number input is then passed through a series of if statements to see which function it correlates with\n",
        "#creates instance of class RecipeOrganizer to access methods defined in the class\n",
        "recipeorganizer = RecipeOrganizer()\n",
        "while True:\n",
        "  print(\"----Recipe Organizer----\")\n",
        "  print(\"1. Add a new recipe\")\n",
        "  print(\"2. Remove a recipe\")\n",
        "  print(\"3. Search for an existing recipe by ingredient\")\n",
        "  print(\"4. Search for an existing recipe by cuisine\")\n",
        "  print(\"5. Get a random existing recipe\")\n",
        "  print(\"6. Exit Program\")\n",
        "  choice = input(\"Enter your choice from 1-6\")\n",
        "\n",
        "\n",
        "  #choice number one calls upon the prompt_recipe function and then associates all variables such as name, cuisine, etc with the input taken in the prompt recipe function\n",
        "  if choice == \"1\":\n",
        "    #associates recipe variable with promp_recipe() function which takes in all the user input for the attributes of class Recipe\n",
        "    recipe = prompt_recipe()\n",
        "    # calls RecipeOrganizer class function add_recipe and passes in recipe\n",
        "    recipeorganizer.add_recipe(recipe)\n",
        "    # Once all the user inputs are taken for the variables of prompt recipe then it will return a \"Recipe was added successfully\"\n",
        "    print(\"Recipe was added successfully\")\n",
        "  #choice number two runs on an inner while loop that will take an input of a recipe name that the user wants to remove\n",
        "  if choice == \"2\":\n",
        "    while True:\n",
        "      name = input(\"Input the name of the recipe you want to remove\").lower()\n",
        "      #once the input is taken the RecipeOrganizer class function remove_recipe will be called and the input will be passed in through that function to remove the recipe\n",
        "      #After the recipe is removed the program will say that the recipe was removed successfully and then the inner loop will break exiting back to the outer loop which is the main menu.\n",
        "      if recipeorganizer.remove_recipe(name) == True:\n",
        "        print(\"Recipe was removed successfully\")\n",
        "        break\n",
        "      else:\n",
        "        print(\"the recipe you entered does not exist\")\n",
        "        break\n",
        "  #choice number 3 will prompt the user to input the name of an ingredient that also runs on an inner while loop\n",
        "  if choice == \"3\":\n",
        "    while True:\n",
        "      ingredient = input(\"input the ingredient of the recipe you are searching for or type done to exit back to main menu\")\n",
        "      #if the user wants to exit back to the main menu then they can input \"done\" which will say \"returning back to main menu\" and break to outer while loop\n",
        "      #I implemented the .lower method so there is no case of upper or lower case difference when the user types \"done\"\n",
        "      if ingredient.lower() == \"done\":\n",
        "        print(\"returning back to main menu\")\n",
        "        break\n",
        "      #The RecipeOrganizer class function search_recipe_ing will be called with the ingredient input passed through it which will then be assigned to variabe \"found_recipes\"\n",
        "      found_recipes = recipeorganizer.search_recipe_ing(ingredient)\n",
        "      if found_recipes != 0:\n",
        "        #the program will then print \"found + how many recipes were found in the search + the search term\"\n",
        "        print(\"Found\", len(found_recipes), \"Recipes with ingredient search term\", ingredient+\".\")\n",
        "        #the program will then print all the recipes found via a for loop\n",
        "        for recipe in found_recipes:\n",
        "         print_recipe(recipe)\n",
        "  #choice number 4 works the same as choice number 3 but instead of serach by ingredient a cuisine type is taken as an input\n",
        "  if choice == \"4\":\n",
        "    while True:\n",
        "      cuisine = input(\"input the cuisine of the recipe you are searching for or type done to exit back to main menu\")\n",
        "      if cuisine.lower() == \"done\":\n",
        "        print(\"returning back to main menu\")\n",
        "        break\n",
        "      found_recipes = recipeorganizer.search_recipe_cuis(cuisine)\n",
        "      if found_recipes != 0:\n",
        "        print(\"Found\", len(found_recipes), \"Recipes with cuisine search term\", cuisine+\".\")\n",
        "      for recipe in found_recipes:\n",
        "        print_recipe(recipe)\n",
        "  #choice number 5 runs on a while loop as well that will prompt the user after a random recipe is selected to continue for another random recipe or not\n",
        "  #this will be the same as the other functions where the user can just type \"done\" and the loop will break to the outer main loop\n",
        "  if choice == \"5\":\n",
        "    while True:\n",
        "        # Check if there are any recipes\n",
        "      if recipeorganizer.recipe_count() == 0:\n",
        "          print(\"There are no recipes to randomly choose from\")\n",
        "      else:\n",
        "        # Get a random recipe\n",
        "        random_recipe = recipeorganizer.get_random_recipe()\n",
        "        # Print the random recipe\n",
        "        print(\"Here is a random recipe:\")\n",
        "        print_recipe(random_recipe)\n",
        "\n",
        "        # Ask for user input to continue or exit\n",
        "        menu = input(\"Generate another random recipe type yes or type 'done' to exit back to the main menu: \")\n",
        "        if menu.lower() == \"done\":\n",
        "          print(\"Returning to the main menu\")\n",
        "          break\n",
        "\n",
        "   #if choice is 6 then the program will say \"thanks for using Recipe Organizer\" and then break the outer loop, exiting the program entirely\n",
        "  if choice == \"6\":\n",
        "    print(\"Thanks for using the Recipe Organizer\")\n",
        "    break\n",
        "\n",
        "\n",
